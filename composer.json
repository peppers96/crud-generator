{
    "name": "diocreation/crud-generator",
    "license": "MIT",
    "description": "Laravel CRUD Generator Tailwind",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Danar Widi",
            "email": "widiu7omo@gmail.com",
            "homepage": "https://diocreation.com"
        }
    ],
    "require": {
        "php": ">=5.6.4|>=7.0",
        "illuminate/support": "^5.3|^6.0|^7.0|^8.0",
        "laravelcollective/html": "^5.3|^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require-dev": {
        "mockery": "^1.0",
        "phpunit": "^5.7|^8.0|^9.0",
        "orchestra/testbench": "^3.3|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "DioCreation\\CrudGenerator\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "DioCreation\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
